{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "../",
    "setupFiles": ["dotenv/config"],
    "setupFilesAfterEnv": ["<rootDir>/config/setupTests.ts"],
    "testEnvironment": "node",
    "testRegex": ".*\\.test-integration\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "verbose": true
  }